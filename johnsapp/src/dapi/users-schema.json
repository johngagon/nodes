{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": ["users"],
  "properties": {
    "users": {
      "$id": "#/properties/users",
      "type": "array",
      "title": "The Users Schema",
      "items": {
        "$id": "#/properties/users/items",
        "type": "object",
        "title": "The Items Schema",
        "required": ["id", "name", "email"],
        "properties": {
          "id": {
            "$id": "#/properties/users/items/properties/id",
            "type": "integer",
            "title": "The Id Schema",
            "default": 0,
            "examples": [1]
          },
          "name": {
            "$id": "#/properties/users/items/properties/name",
            "type": "string",
            "title": "The Name Schema",
            "default": "",
            "examples": ["John Blue"],
            "pattern": "^(.*)$"
          },
          "email": {
            "$id": "#/properties/users/items/properties/email",
            "type": "string",
            "title": "The Email Schema",
            "default": "",
            "examples": ["john.blue12345@gmailese.com"],
            "pattern": "^(.*)$"
          }
        }
      }
    }
  }
}
